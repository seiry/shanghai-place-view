generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-1.1.x"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model log {
  logid  Int      @id @default(autoincrement()) @db.UnsignedInt
  spotid Int      @db.UnsignedSmallInt
  num    Int
  daynum Int
  time   DateTime @db.DateTime(0)
  spot   spot     @relation(fields: [spotid], references: [spotid], onDelete: NoAction, onUpdate: NoAction, map: "realname")

  @@index([time], map: "time")
  @@index([spotid], map: "realname")
}

model spot {
  spotid Int    @id @db.UnsignedSmallInt
  name   String @default("") @db.VarChar(255)
  log    log[]

  @@index([spotid, name], map: "spotid")
}
