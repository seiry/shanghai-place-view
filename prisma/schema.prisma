generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model log {
  logid     Int       @id @default(autoincrement()) @db.UnsignedInt
  jsonid    String?   @db.Text
  time      DateTime? @db.DateTime(0)
  spotId    Int?
  spotName  String?   @db.Text
  num       Int?      @db.UnsignedInt
  maxnum    Int?
  closeTime String?   @db.Text

  @@index([time], map: "time")
}
